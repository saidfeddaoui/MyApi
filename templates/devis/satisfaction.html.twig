{% extends 'base.html.twig' %}
{% block javascripts %}
    {{ parent() }}
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="{{ asset('js/devis_satisfaction.js') }}" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN EXAMPLE TABLE PORTLET-->
            <div class="portlet light portlet-fit ">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-settings font-red"></i>
                        <span class="caption-subject font-red sbold uppercase">Devis Satisfaisants</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="table-toolbar">
                        <div class="row">
                            <div class="col-md-6">

                            </div>
                        </div>
                    </div>
                    <table class="table table-striped table-hover table-bordered datatable" id="sample_editable_1">
                        <thead>
                        <tr>
                            <th> Type devis  </th>
                            <th> Nom </th>
                            <th> Prénom </th>
                            <th> Téléphone </th>
                            <th> Pack </th>
                            <th> Raison </th>
                            <th> Commentaire </th>
                            <th> Observation </th>
                            <th> Agence </th>
                            <th> Action </th>
                            <th> Date de création  </th>
                            <th> Statut </th>
                        </tr>
                        </thead>
                        <tbody>
                        {% for element in data %}
                          {% if  element.auto=="DA" %}
                        <tr class="devis_satisfaction_details" data-id="{{ element.id }}">
                            <td> AUTO </td>

                            {% if element.devisAuto is not null %}
                                <td> {{ element.devisAuto.nom }} </td>
                            {% else %}
                                <td> Pas disponible </td>
                            {% endif %}

                            {% if element.devisAuto is not null %}
                                <td> {{ element.devisAuto.prenom }} </td>
                            {% else %}
                                <td> Pas disponible </td>
                            {% endif %}

                            {% if  element.devisAuto is not null %}
                                <td>  {{ element.devisAuto.tel }} </td>
                            {% else %}
                                <td> Pas disponible </td>
                            {% endif %}

                            {% if  element.devisAuto.pack %}
                                <td>  {{ element.devisAuto.pack.name }} </td>
                            {% else  %}
                                <td> Pas diponible </td>
                            {% endif %}

                            {% if  element.raison is not null %}
                                <td>  {{ element.raison.title }} </td>
                            {% else %}
                                <td> Pas disponible </td>
                            {% endif %}

                            {% if  element.comment is not null %}
                                <td> {{ element.comment }} </td>
                            {% else %}
                                <td>Pas disponible </td>
                            {% endif %}

                            {% if  element.observation is not null %}
                                <td> {{ element.observation }} </td>
                            {% else %}
                                <td> </td>
                            {% endif %}
                            {% if  element.agence is not null %}
                                <td> {{ element.agence }} </td>
                            {% else %}
                                <td> </td>
                            {% endif %}


                            {% if  element.action is not null %}
                                <td>
                                {% if element.action == 2 %}
                                    <p class="text-success">Contacté</p>
                                {% elseif element.action == 1 %}
                                    <p class="text-warning">Déjà appelé</p>
                                {% elseif element.action == 0 %}
                                    <p class="text-error">Injoignable</p>
                                {% else %}
                                    <p>Aucune Action</p>
                                {% endif %}
                                </td>
                            {% else %}
                                <td>  </td>
                            {% endif %}

                            {% if element.dateCreation is not null %}
                            <td> {{ element.dateCreation | date('Y-m-d H:i:s')  }} </td>
                            {% else %}
                             <td> Pas disponible </td>
                            {% endif %}

                            {% if  element.statut   %}
                                <td> <i class="icon-like emoLike "></i></td>
                            {% else %}
                                <td> <i class="icon-dislike emodisLike"></i> </td>
                            {% endif %}
                         </tr>
                      {% elseif  element.auto=="DH" %}
                          <tr class="devis_satisfaction_details" data-id="{{ element.id }}">
                              <td> MRH </td>

                              {% if element.devisHabitation is not null %}
                                  <td> {{ element.devisHabitation.nom }} </td>
                              {% else %}
                                  <td> Pas disponible </td>
                              {% endif %}

                              {% if element.devisHabitation is not null %}
                                  <td> {{ element.devisHabitation.prenom }} </td>
                              {% else %}
                                  <td> Pas disponible </td>
                              {% endif %}

                              {% if  element.devisHabitation is not null %}
                                  <td>  {{ element.devisHabitation.tel }} </td>
                              {% else %}
                                  <td> Pas disponible </td>
                              {% endif %}

                              <td> Pas de Pack </td>

                              {% if  element.raison is not null %}
                                  <td>  {{ element.raison.title }} </td>
                              {% else %}
                                  <td> Pas disponible </td>
                              {% endif %}


                              {% if  element.comment is not null %}
                                  <td> {{ element.comment }} </td>
                              {% else %}
                                  <td>Pas disponible </td>
                              {% endif %}

                              {% if  element.observation is not null %}
                                  <td> {{ element.observation }} </td>
                              {% else %}
                                  <td>Pas disponible </td>
                              {% endif %}

                              {% if  element.action is not null %}
                                  <td>
                                  {% if element.action == 2 %}
                                      <p class="text-success">Contacté</p>
                                  {% elseif element.action == 1 %}
                                      <p class="text-warning">Déjà appelé</p>
                                  {% elseif element.action == 0 %}
                                      <p class="text-danger">Injoignable</p>
                                  {% else %}
                                      <p>Aucune Action</p>
                                  {% endif %}
                                  </td>
                              {% else %}
                                  <td>Pas action </td>
                              {% endif %}
                              <td>Pas action </td>

                              {% if element.dateCreation is not null %}
                                  <td> {{ element.dateCreation | date('Y-m-d H:i:s')  }} </td>
                              {% else %}
                                  <td> Pas disponible </td>
                              {% endif %}

                              {% if  element.statut   %}
                                  <td> <i class="icon-like emoLike "></i></td>
                              {% else %}
                                  <td> <i class="icon-dislike emodisLike"></i> </td>
                              {% endif %}
                          </tr>
                       {% endif %}
                        {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->
        </div>
    </div>
{% endblock %}
