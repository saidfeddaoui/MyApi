{% extends 'base.html.twig' %}
{% block javascripts %}
    {{ parent() }}
    <!-- BEGIN PAGE LEVEL SCRIPTS -->

    <!-- END PAGE LEVEL SCRIPTS -->
{% endblock %}
{% block body %}
    <div class="row">
        <div class="col-md-12">
            <!-- BEGIN EXAMPLE TABLE PORTLET-->
            <div class="portlet light portlet-fit ">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-settings font-red"></i>
                        <span class="caption-subject font-red sbold uppercase">Devis</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class="table-toolbar">
                        <div class="row">
                            <div class="col-md-6">
                                <p>
                                    <span class="detail_label">Police : </span>
                                    <span class="detail_info">
                                        {% if devisAuto.nom is not null %}
                                            <input type="text" class="police" name="nom" disabled value="{{ devisAuto.nom }}">
                                        {% else %}
                                            <input type="text" class="police" name="nom" disabled value="">
                                        {% endif %}
                                     </span>
                                </p>

                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->
        </div>
    </div>
{% endblock %}
